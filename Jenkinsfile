#!/usr/bin/env groovy
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
            
                echo 'Building Project Geo'
                echo 'Build ID: ${env.BUILD_ID}'
                echo 'Build ID:' + BUILD_ID
                echo 'Build ID: ${env.PLATFORM}'
                echo 'Platform:' + PLATFORM
                
            }
        }
        
    }
}